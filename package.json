{
  "name": "toml-matter",
  "version": "1.0.0",
  "description": "Frontmatter parser for TOML in +++ delimiters.",
  "author": {
    "name": "Colin Bate",
    "email": "colin@colinbate.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/colinbate/toml-matter"
  },
  "keywords": ["toml", "markdown", "frontmatter", "front", "matter", "parse", "blog"],
  "type": "module",
  "main": "./dist/toml-matter.umd.cjs",
  "module": "./dist/toml-matter.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/toml-matter.js",
      "require": "./dist/toml-matter.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "fix": "eslint --fix"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.2"
  },
  "dependencies": {
    "toml": "^3.0.0"
  }
}
